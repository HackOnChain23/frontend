#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kamilpaluchowski/Desktop/hoc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/kamilpaluchowski/Desktop/hoc/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@intlify+vue-router-bridge@0.8.0/node_modules/@intlify/vue-router-bridge/bin/switch.js" "$@"
else
  exec node  "$basedir/../../../../../@intlify+vue-router-bridge@0.8.0/node_modules/@intlify/vue-router-bridge/bin/switch.js" "$@"
fi
